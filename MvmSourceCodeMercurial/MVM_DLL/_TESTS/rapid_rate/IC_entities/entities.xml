  <entity>
    <name>PARAM_TABLE</name>
    <operation name="metadata">param_table_structure</operation>
    <dynamic_operation name="put">param_table_put</dynamic_operation>
    <dynamic_operation name="get">get_rscheds</dynamic_operation>
  </entity>
</entity_config>
